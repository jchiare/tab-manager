{
  "manifest_version": 3,
  "name": "Tab Cleaner",
  "version": "1.0",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": ["tabs", "storage", "scripting"],
  "description": "Deduplicate tabs",
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker":"serviceWorker.js"
  },
  "icons": {
    "48": "icon48.png",
    "96": "icon96.png",
    "128": "icon128.png"
  }
}
